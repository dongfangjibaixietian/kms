{
  "name": "gworld-pc-share",
  "version": "1.0.0",
  "description": "Gworld 知识库 知识分享平台",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config build/webpack.config.js --mode development",
    "commit": "git add . && git-cz",
    "init:cz": "commitizen init cz-conventional-changelog --save --save-exact",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --fix && stylelint \"./src/**/*.scss\"",
    "build:dev": "cross-env NODE_ENV=development webpack --config build/webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config build/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "http://172.16.1.253:3537/gworld/gworld-pc-share.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "antd": "^4.2.4",
    "axios": "^0.19.2",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^2.0.7",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@svgr/webpack": "^5.4.0",
    "@types/node": "^14.0.1",
    "@types/qs": "^6.9.2",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "cache-loader": "^4.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "css-modules-typescript-loader": "^4.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "html-webpack-plugin": "^4.3.0",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "lint-staged": "^10.2.4",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.4.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.17.2",
    "terser-webpack-plugin": "^3.0.1",
    "thread-loader": "^2.1.3",
    "typescript": "^3.9.2",
    "url-loader": "^4.1.0",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "yorkie": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "build",
        "ci",
        "chore",
        "revert"
      ],
      "scope": {
        "required": false,
        "allowed": [
          "*"
        ],
        "validate": false,
        "multiple": false
      },
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "提交信息不符合提交规范，请重新提交！",
      "helpMessage": "",
      "autoFix": false
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "validate-commit-msg"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts}": [
      "npm run lint",
      "git add"
    ]
  }
}
